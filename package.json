{
  "name": "markedpp-ninja",
  "version": "2.0.1",
  "description": "Preprocessor for markdown files",
  "keywords": [
    "autonumber ordered lists",
    "include files",
    "markdown",
    "number headings",
    "preprocessor",
    "table of contents",
    "toc"
  ],
  "homepage": "https://github.com/gatewayprogrammingschool/markedpp",
  "bugs": {
    "url": "https://github.com/gatewayprogrammingschool/markedpp/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatewayprogrammingschool/markedpp.git"
  },
  "license": "MIT",
  "author": "The Sharp Ninja <ninja@thesharp.ninja>, commenthol <commenthol@gmail.com>",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "bin": {
    "markedpp": "./bin/markedpp.js"
  },
  "man": "./man/markedpp.1",
  "scripts": {
    "all": "npm-run-all clean test build",
    "build": "webpack",
    "clean": "rimraf doc coverage .nyc_output dist lib",
    "coverage": "nyc -r text -r html npm test",
    "doc": "mkdir -p doc && jsdox -o doc src/*.js",
    "man": "make man",
    "prepublishOnly": "npm run all",
    "readme": "make readme",
    "test": "mocha",
    "test:browser": "make browser"
  },
  "babel": {
    "ignore": [
      "./dist"
    ],
    "plugins": [],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "asyncc": "^2.0.4",
    "emoji-regex": "^9.2.0",
    "html-entities": "^2.0.2",
    "plantuml-api": "0.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@tryghost/kg-markdown-html-renderer": "^3.0.0",
    "babel-loader": "^8.2.2",
    "cheerio": "1.0.0-rc.3",
    "eslint": "^7.17.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "express": "^4.17.1",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "marked": "^2.0.3",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "parallel-webpack": "^2.6.0",
    "rehype-format": "^3.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-html": "^13.0.1",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-slug": "^6.0.0",
    "resolve-cwd": "^2.0.0",
    "rimraf": "^3.0.2",
    "superagent": "^6.1.0",
    "unified": "^9.2.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "maintainers": [
    "The Sharp Ninja <ninja@thesharp.ninja>",
    "commenthol <commenthol@gmail.com>"
  ]
}
